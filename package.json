{
  "name": "calenconnect-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/calenconnect-api/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest --config=jest.config.js",
    "test:unit": "NODE_ENV=test jest --selectProjects unit --config=jest.config.js",
    "test:integration": "NODE_ENV=test jest --selectProjects integration --config=jest.config.js",
    "test:e2e": "NODE_ENV=test jest --selectProjects e2e --config=jest.config.js",
    "test:watch": "NODE_ENV=test jest --watch --config=jest.config.js",
    "test:cov": "NODE_ENV=test jest --coverage --config=jest.config.js",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:module": "NODE_ENV=test jest --config=jest.config.js --testMatch=\"**/__tests__/**/*.spec.ts\"",
    "test:module:unit": "NODE_ENV=test jest --config=jest.config.js --testMatch=\"**/__tests__/unit/**/*.spec.ts\"",
    "test:module:integration": "NODE_ENV=test jest --config=jest.config.js --testMatch=\"**/__tests__/integration/**/*.spec.ts\"",
    "test:module:e2e": "NODE_ENV=test jest --config=jest.config.js --testMatch=\"**/__tests__/e2e/**/*.spec.ts\"",
    "test:availability": "NODE_ENV=test jest --config=jest.config.js --testMatch=\"**/availability/**/*.spec.ts\"",
    "test:appointment": "NODE_ENV=test jest --config=jest.config.js --testMatch=\"**/appointment/**/*.spec.ts\"",
    "test:user": "NODE_ENV=test jest --config=jest.config.js --testMatch=\"**/user/**/*.spec.ts\"",
    "seed": "ts-node prisma/seed.ts",
    "seed:test": "NODE_ENV=test ts-node prisma/seed-test.ts",
    "db:test:prepare": "ts-node prisma/prepare-test-db.ts"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/swagger": "^11.0.6",
    "@supabase/supabase-js": "^2.49.1",
    "@types/js-yaml": "^4.0.9",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express": "^5.0.1",
    "js-yaml": "^4.1.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@prisma/client": "^6.4.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.8",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.4.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}
